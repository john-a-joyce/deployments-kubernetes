kind load docker-image calico/cni:v3.25.0
kind load docker-image calico/pod2daemon-flexvol:v3.25.0
kind load docker-image calico/node:v3.25.0
kind load docker-image calico/kube-controllers:v3.25.0
curl https://raw.githubusercontent.com/projectcalico/calico/v3.25.0/manifests/calico.yaml -O
kubectl apply -f calico.yaml
